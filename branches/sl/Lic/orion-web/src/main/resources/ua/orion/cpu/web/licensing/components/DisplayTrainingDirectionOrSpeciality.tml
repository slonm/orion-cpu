<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<div xml:space="default" 
     xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
     xmlns:p="tapestry:parameter">
    <t:if test="IsTrainingDirection">
        ${licenseRecord.TrainingDirection.name}
        <p:else>
            <t:if test="licenseRecord.Speciality">
                ${licenseRecord.Speciality.name}
                <p:else>
                    -
                </p:else>
            </t:if>
        </p:else>
    </t:if>
    <t:if test="showVariants">
        <t:ori.Spoiler t:link="prop:licenseRecord.classify">
            <p t:type="loop" t:source="trainingVariants" t:value="trainingVariant">
                ${trainingVariant}
            </p>
        </t:ori.Spoiler> 
    </t:if>    
</div>

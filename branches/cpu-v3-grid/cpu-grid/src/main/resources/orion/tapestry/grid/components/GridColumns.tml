    <tr xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd" xml:space="default">
        <th t:type="Loop" source="columnList" id="${columnModel.id}ColumnHeader" value="columnModel" formstate="none" index="index" valign="top">
            <t:delegate to="blockForColumn"/>
            <t:block id="standardHeader">
                <div class="ColumnHeaderText">${columnModel.label}</div>
            </t:block>
        </th>
    </tr>

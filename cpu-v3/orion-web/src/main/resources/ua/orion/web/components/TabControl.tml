<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<div id="${clientId}" class="ori_tabControl" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">

    <div t:type="Loop" source="panelIds" value="panelId"
          id="${clientId}_${panelId}"
          class="ori-tab ui-state-default ui-corner-tl ui-corner-tr">
        <t:eventLink context="${panelId}" 
                     event="select" zone="${contentZone}">
            ${panelTitle}
        </t:eventLink>
    </div>
</div>
